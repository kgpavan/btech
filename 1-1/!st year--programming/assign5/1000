		350 235 6
		440 10 10
		9 200 4
		100 235 10
		685 786 7
		200 40 5
		6 200 20
		for(j=0;j<n;j++)
		{
			q[i][j]=0;
		}
	}
for(i=0;i<c;i++)
{
	for(j=0;j<d;j++)
	{
		if(a[i][1]==b[j][0])
		{
			a[i][0]=r;
			b[j][1]=s;
			q[r][s]=(q[r][s]+(a[i][2]*b[j][2]));
		}
	}
}
for(i=0;i<n;i++)
{
	for(j=0;j<n;j++)
	{
		if(q[i][j]!=0)
		{
			printf("%d %d %d\n",r,s,q[r][s]);
		}
	}
}
}
