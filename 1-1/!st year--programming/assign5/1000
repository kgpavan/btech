#include<stdio.h>
main()
{
	int n,r,s,i,j;
	scanf("%d",&n);
	int c,d,q[n][n];
	scanf("%d",&c);
	int a[c][3];
	for(i=0;i<c;i++)
	{
		for(j=0;j<3;j++)
		{
			scanf("%d",&a[i][j]);
		}
	}
	scanf("%d",&d);
	int b[d][3];
	for(i=0;i<d;i++)
	{
		for(j=0;j<3;j++)
		{
			scanf("%d",&b[i][j]);
		}
	}
	for(i=0;i<n;i++)
	{
	1000
		9
		100 200 6
		5 100 15
		891 682 75
		5 6 10
		200 440 7
		5 350 20
		791 482 75
		5 9 5
		300 981 10	
		7
		350 235 6
		440 10 10
		9 200 4
		100 235 10
		685 786 7
		200 40 5
		6 200 20
		1000
		9
		100 200 6
		5 100 15
		891 682 75
		5 6 10
		200 440 7
